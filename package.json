{
  "name": "xpenzi",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\" --write",
    "format:check": "prettier \"**/*.{ts,tsx,js,jsx,json,md,css,scss}\" --check",
    "test": "pnpm -r run test",
    "dev:web": "pnpm --filter web dev",
    "dev:lambdas:offline": "pnpm --filter lambdas dev",
    "seed": "pnpm --filter db seed"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "services",
    "infra"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "^5.9.3"
  }
}
